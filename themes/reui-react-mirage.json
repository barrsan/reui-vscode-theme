  "tokenColors": [
    {
      "name": "keywords",
      "scope": ["keyword", "storage.type.class"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c5a5c5"
      }
    },
    {
      "name": "strings",
      "scope": "string",
      "settings": {
        "foreground": "#8dc891"
      }
    },
    {
      "name": "functions",
      "scope": "function",
      "settings": {
        "foreground": "#79b6f2"
      }
    },
    {
      "name": "variables",
      "scope": ["variable", "meta.jsx.children"],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "types",
      "scope": [
        "type",
        "support.type",
        "support.type.property-name.json.comments"
      ],
      "settings": {
        "foreground": "#fac863"
      }
    },
    {
      "name": "Name.Constant",
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#8be9ee"
      }
    },
    {
      "name": "Generic: Heading (multi line)",
      "scope": "markup.heading.1 punctuation.definition.heading",
      "settings": {
        "foreground": "#ff0000"
      }
    },
    {
      "name": "Name.Entity",
      "scope": "constant.character.entity",
      "settings": {
        "foreground": "#d67c9b"
      }
    },
    {
      "name": "Name.Exception",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#d67c9b"
      }
    },
    {
      "name": "Name.Function",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.class.builtin",
        "keyword.other.name-of-parameter"
      ],
      "settings": {
        "foreground": "#79b6f2"
      }
    },
    {
      "name": "Name.Tag",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#8be9ee"
      }
    },
    {
      "name": "Support.Class",
      "scope": [
        "support.class",
        "support.class.component",
        "constant.numeric",
        "punctuation.terminator.statement",
        "support.variable.property.dom"
      ],
      "settings": {
        "foreground": "#fc929e"
      }
    },
    {
      "name": "Component",
      "scope": ["entity.other.inherited-class", "entity.name.type"],
      "settings": {
        "foreground": "#fac863"
      }
    },
    {
      "name": "Storage.Modifier",
      "scope": ["storage.modifier", "storage.type"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c5a5c5"
      }
    },
    {
      "name": "Storage.Type",
      "scope": ["constructor"],
      "settings": {
        "foreground": "#79b6f2"
      }
    },
    {
      "name": "Variable.Language",
      "scope": [
        "variable.language",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#c5a5c5",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Boolean & Null",
      "scope": ["constant.language.boolean", "constant.language.null", "constant.language.undefined"],
      "settings": {
        "foreground": "#ff8b50",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Parameter",
      "scope": [
        "variable.parameter",
        "variable.other.readwrite",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Constant",
      "scope": ["variable.other.constant"],
      "settings": {
        "foreground": "#27E2F1"
      }
    },
    {
      "name": "Object Property",
      "scope": ["variable.other.property"],
      "settings": {
        "foreground": "#beedff"
      }
    },
    {
      "name": "Object Key",
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#8ADF69"
      }
    },
    {
      "name": "Object Key 2",
      "scope": ["meta.var.expr meta.objectliteral meta.object.member meta.objectliteral meta.object.member meta.object-literal.key"],
      "settings": {
        "foreground": "#caffb5"
      }
    },
    {
      "name": "Constant.Object.Keys",
      "scope": ["string.unquoted", "constant.other.object.key"],
      "settings": {
        "foreground": "#8ADF69"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "punctuation.definition.tag",
        "punctuation.section.embedded",
        "meta.brace.round",
        "meta.array.literal"
      ],
      "settings": {
        "foreground": "#88c6be"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#F38E9A",
        "fontStyle": ""
      }
    },
    {
      "name": "Operator New",
      "scope": ["keyword.operator.new"],
      "settings": {
        "foreground": "#F38E9A",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.line",
        "comment.line.double-slash"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#797979"
      }
    },
    {
      "name": "Class.Name",
      "scope": ["entity.name.class"],
      "settings": {
        "fontStyle": "",
        "foreground": "#fac863"
      }
    },
    {
      "name": "Decorator",
      "scope": [
        "entity.name.class.decorator",
        "variable.other.readwrite.decorator"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#88c6be"
      }
    },
    {
      "name": "Variable.Other.Class",
      "scope": ["variable.other.class", "meta.property.class"],
      "settings": {
        "foreground": "#A4FFFF"
      }
    },
    {
      "name": "Function.Arrow",
      "scope": ["storage.type.function.arrow", "meta.function.arrow"],
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Dart.MainText",
      "scope": [
        "source.dart"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Dart.ComparisonOperator",
      "scope": [
        "keyword.operator.comparison.dart"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Dart.ArithmeticOperator",
      "scope": [
        "keyword.operator.arithmetic.dart"
      ],
      "settings": {
        "foreground": "#fac863"
      }
    },
    {
      "name": "Dart.Constant",
      "scope": [
        "constant.language.dart"
      ],
      "settings": {
        "foreground": "#ff8b50",
        "fontStyle": "italic"
      }
    },
